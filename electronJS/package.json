{
    "name": "fast-spider",
    "productName": "FastSpider",
    "version": "0.0.1",
    "icon": "./favicon",
    "description": "Web Crawler Without Code",
    "main": "./dist/electron/main/index.js",
    "scripts": {
        "dev": "vite --port 3003 --strictPort",
        "build": "npm run build:electron && npm run build:renderer",
        "build:renderer": "vite build",
        "build:electron": "tsc --project tsconfig.electron.json",
        "preview": "vite preview",
        "start": "electron .",
        "electron:dev": "concurrently \"npm run dev\" \"powershell -Command wait-on http://localhost:3003; npm run start\"",
        "electron:build": "npm run build && electron-builder",
        "type-check": "vue-tsc --noEmit",
        "clean": "powershell -Command \"if (Test-Path 'release') { Remove-Item -Recurse -Force release }; if (Test-Path 'dist') { Remove-Item -Recurse -Force dist }\""
    },
    "keywords": [
        "Electron",
        "Vue",
        "TypeScript",
        "Vite",
        "爬虫"
    ],
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/node": "^20.8.7",
        "@types/selenium-webdriver": "^4.35.4",
        "@types/ws": "^8.18.1",
        "concurrently": "^8.2.2",
        "electron": "^27.1.3",
        "electron-builder": "^24.6.4",
        "electron-is-dev": "^2.0.0",
        "electron-reloader": "^1.2.3",
        "electron-vite": "^3.1.0",
        "typescript": "^4.9.5",
        "vue-tsc": "^1.0.24",
        "wait-on": "^7.2.0"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.2",
        "@types/mysql": "^2.15.27",
        "@vitejs/plugin-vue": "^4.2.3",
        "bootstrap": "^5.3.0",
        "cors": "^2.8.5",
        "electron-squirrel-startup": "^1.0.0",
        "element-plus": "^2.11.7",
        "express": "^4.21.2",
        "formidable": "^3.5.0",
        "http": "^0.0.1-security",
        "i18next": "^21.9.2",
        "i18next-browser-languagedetector": "^6.1.4",
        "jquery": "^3.7.1",
        "multer": "^2.0.0",
        "mysql2": "^3.15.3",
        "node-abi": "^3.52.0",
        "selenium-webdriver": "^4.27.0",
        "vite": "^4.4.0",
        "vue": "^3.3.0",
        "vue-i18n": "^9.4.0",
        "vue-router": "^4.2.0",
        "vxe-table": "^4.7.17",
        "ws": "^8.18.0",
        "xlsx": "^0.18.5"
    }
}
