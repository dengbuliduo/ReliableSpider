{
    "name": "fast-spider",
    "productName": "FastSpider",
    "version": "0.0.1",
    "icon": "./favicon",
    "description": "Web Crawler Without Code",
    "main": "main.js",
    "scripts": {
        "start_direct": "electron .",
        "change_version": "node change_version.js",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "dev:electron": "vite && electron .",
        "build:electron": "vite build && electron .",
        "serve:electron": "npm run build && electron .",
        "dev:reload": "NODE_ENV=development electron .",
        "dev:reload:win": "set NODE_ENV=development && electron ."
    },
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "devDependencies": {
        "@electron-forge/cli": "^6.0.5",
        "@electron-forge/maker-deb": "^6.0.5",
        "@electron-forge/maker-rpm": "^6.0.5",
        "@electron-forge/maker-squirrel": "^6.0.5",
        "@electron-forge/maker-zip": "^6.0.5",
        "electron": "^27.1.3",
        "electron-reloader": "^1.2.3"
    },
    "repository": "https://github.com/dengbuliduo/FastSpider",
    "dependencies": {
        "@element-plus/icons-vue": "^2.3.2",
        "@vitejs/plugin-vue": "^4.2.3",
        "bootstrap": "^5.3.0",
        "cors": "^2.8.5",
        "electron-squirrel-startup": "^1.0.0",
        "element-plus": "^2.11.7",
        "express": "^4.21.2",
        "formidable": "^3.5.0",
        "http": "^0.0.1-security",
        "i18next": "^21.9.2",
        "i18next-browser-languagedetector": "^6.1.4",
        "jquery": "^3.7.1",
        "multer": "^2.0.0",
        "node-abi": "^3.52.0",
        "node-window-manager": "^2.2.4",
        "selenium-webdriver": "^4.27.0",
        "vite": "^4.4.0",
        "vue": "^3.3.0",
        "vue-i18n": "^9.4.0",
        "vue-router": "^4.2.0",
        "vxe-table": "^4.7.17",
        "ws": "^8.18.0",
        "xlsx": "^0.18.5"
    },
    "config": {
        "forge": {
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ReliableSpider"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb"
                },
                {
                    "name": "@electron-forge/maker-rpm"
                }
            ],
            "packagerConfig": {
                "icon": "./favicon",
                "appVersion": "0.0.1",
                "name": "FastSpider",
                "executableName": "FastSpider",
                "appCopyright": "dengbuliduo",
                "win32metadata": {
                    "ProductName": "FastSpider",
                    "FileDescription": "FastSpider --- 一个快速的不需编码的爬虫工具"
                }
            },
            "publishers": []
        }
    }
}
