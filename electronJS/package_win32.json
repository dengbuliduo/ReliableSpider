{
    "name": "reliable-Spider",
    "productName": "ReliableSpider",
    "version": "0.6.0",
    "icon": "./favicon",
    "description": "NoCode Visual Web Crawler",
    "main": "main.js",
    "scripts": {
        "start_direct": "electron .",
        "change_version": "node change_version.js",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make"
    },
    "keywords": [
        "Electron",
        "quick",
        "start",
        "tutorial",
        "demo"
    ],
    "author": "dengbuliduo",
    "license": "AGPL-3.0",
    "devDependencies": {
        "@electron-forge/cli": "^6.0.5",
        "@electron-forge/maker-deb": "^6.0.5",
        "@electron-forge/maker-rpm": "^6.0.5",
        "@electron-forge/maker-squirrel": "^6.0.5",
        "@electron-forge/maker-zip": "^6.0.5",
        "electron": "^20.0.1"
    },
    "repository": "https://github.com/dengbuliduo/ReliableSpider",
    "dependencies": {
        "cors": "^2.8.5",
        "electron-squirrel-startup": "^1.0.0",
        "express": "^4.18.2",
        "formidable": "^3.5.0",
        "http": "^0.0.1-security",
        "multer": "^1.4.5-lts.1",
        "node-abi": "^3.52.0",
        "node-window-manager": "^2.2.4",
        "selenium-webdriver": "^4.16.0",
        "ws": "^8.12.0",
        "xlsx": "^0.18.5"
    },
    "config": {
        "forge": {
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "ReliableSpider"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb"
                },
                {
                    "name": "@electron-forge/maker-rpm"
                }
            ],
            "packagerConfig": {
                "icon": "./favicon",
                "appVersion": "0.6.0",
                "name": "ReliableSpider",
                "executableName": "ReliableSpider",
                "appCopyright": "dengbuliduo"
            },
            "publishers": []
        }
    }
}
